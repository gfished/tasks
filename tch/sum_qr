#include <iostream>
#include <cmath>

using namespace std;

int f(int n)
{
    int counter{0};
    for (int a = 0; a*a < n; ++a)
    {
        int tmp = sqrt(n - a*a);
        if (tmp*tmp == (n - a*a)) ++counter;
    }
    return counter;
}

int main()
{
    int n{0};
    cin >> n;
    for (int i = 0; i < n; ++n)
    {
        int a{0};
        cout << f(a);
    }
}
